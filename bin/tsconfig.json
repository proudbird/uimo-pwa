{
  "compilerOptions": {
    "target": "ES2020",
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "noEmitOnError": false,
    "noEmit": false,
    "outDir": "./build",
  },
  "include": [
    ".", 
  ],
  "exclude": [ "build"]
}
